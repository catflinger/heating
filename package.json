{
  "name": "heating-controller",
  "version": "0.0.0",
  "description": "heating controller model",
  "license": "MIT",
  "repository": "",
  "author": "",
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "cls && npm run build && mocha --compilers ts:ts-node/register --recursive \"test/**/*-spec.ts\"",
    "testa": "cls && npm run build && mocha --compilers ts:ts-node/register --recursive \"test/program-store/program-store-spec.ts\"",
    "coverage": "nyc --include='src/**/*.ts' --reporter=text --reporter=html --reporter=lcov mocha --compilers ts:ts-node/register --recursive \"test/**/*.test.ts\"",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "start": "npm run build && set DEBUG=app,dump && node lib/src/index.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "*",
    "inversify": "^4.13.0",
    "moment": "^2.22.2",
    "node-schedule": "1.3.0",
    "reflect-metadata": "^0.1.10",
    "debug": "^3.2.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^3.5.0",
    "@types/chai-http": "^3.0.5",
    "@types/debug": "0.0.29",
    "@types/expect.js": "^0.3.29",
    "@types/express": "^4.16.0",
    "@types/mocha": "^2.0.0",
    "@types/moment": "^2.13.0",
    "@types/node": "10.11.3",
    "@types/node-schedule": "^1.2.2",
    "@types/rimraf": "^2.0.0",
    "@types/uuid": "^3.4.4",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.0.0",
    "expect.js": "^0.3.1",
    "mocha": "^3.0.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.0.0",
    "ts-node": "^3.0.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
